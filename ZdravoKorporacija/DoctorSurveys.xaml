        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bolnica"
        mc:Ignorable="d"
        Loaded="OnLoad"
        Title="Doctor_Surveys" Height="450" Width="500">
